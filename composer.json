{
    "name": "nystudio107/craft-retour",
    "description": "Retour allows you to intelligently redirect legacy URLs, so that you don't lose SEO value when rebuilding & restructuring a website",
    "type": "craft-plugin",
    "version": "3.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "redirect",
        "retour"
    ],
    "support": {
        "docs": "https://github.com/nystudio107/retour/blob/v1/README.md",
        "issues": "https://github.com/nystudio107/retour/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "nystudio107",
            "homepage": "https://nystudio107.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "nystudio107\\retour\\": "src/"
        }
    },
    "extra": {
        "name": "Retour",
        "handle": "retour",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/nystudio107/retour/v1/CHANGELOG.md",
        "components": {
            "redirects": "nystudio107\\retour\\services\\Redirects"
        },
        "class": "nystudio107\\retour\\Retour"
    }
}
