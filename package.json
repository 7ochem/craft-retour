{
    "name": "retour",
    "version": "1.0.0",
    "description": "Retour allows you to intelligently redirect legacy URLs, so that you don't lose SEO value when rebuilding & restructuring a website",
    "copyright": "nystudio107",
    "main": "index.js",
    "scripts": {
        "dev": "webpack-dev-server --config webpack.dev.js --progress --hide-modules",
        "build": "webpack --config webpack.prod.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nystudio107/craft-retour.git"
    },
    "author": "andrew@nystudio107.com",
    "license": "Craft License",
    "bugs": {
        "url": "https://github.com/nystudio107/craft-retour/issues"
    },
    "homepage": "https://github.com/nystudio107/craft-retour",
    "paths": {
        "src": {
            "base": "./src/assetbundles/retour/src/",
            "js": "./src/assetbundles/retour/src/js/",
            "css": "./src/assetbundles/retour/src/css/"
        },
        "dist": {
            "base": "./src/assetbundles/retour/dist/",
            "public": "/cpresources/retour/",
            "devPublic": "http://192.168.10.10:8080",
            "clean": [
                "./js",
                "./css"
            ]
        },
        "copyFiles": [],
        "manifest": {
            "basePath": ""
        },
        "templates": "./src/templates/"
    },
    "entries": {
        "dashboard": "Dashboard.js",
        "import": "Import.js",
        "redirects": "Redirects.js",
        "retour": "Retour.js",
        "widget": "Widget.js"
    },
    "babelConfig": {
        "legacyBrowsers": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR"
        ],
        "modernBrowsers": [
            "last 2 Chrome versions",
            "not Chrome < 60",
            "last 2 Safari versions",
            "not Safari < 10.1",
            "last 2 iOS versions",
            "not iOS < 10.3",
            "last 2 Firefox versions",
            "not Firefox < 54",
            "last 2 Edge versions",
            "not Edge < 15"
        ]
    },
    "purgeCss": {
        "paths": [
            "./src/templates/**/*.{twig,html}",
            "./node_modules/vuetable-2/src/components/**/*.{vue,html}",
            "./src/assetbundles/retour/src/vue/**/*.{vue,html}"
        ],
        "whitelist": [
            "./src/assetbundles/retour/src/css/components/tables.css"
        ],
        "whitelistPatterns": [],
        "extensions": [
            "html",
            "js",
            "twig",
            "vue"
        ]
    },
    "urls": {},
    "vars": {
        "cssName": "styles",
        "jsName": "retour"
    },
    "devDependencies": {
        "@babel/core": "^7.1.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/register": "^7.0.0",
        "@babel/runtime": "^7.0.0",
        "autoprefixer": "^9.1.5",
        "babel-loader": "^8.0.2",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^2.0.0",
        "git-rev-sync": "^1.12.0",
        "glob-all": "^3.1.0",
        "ignore-loader": "^0.1.2",
        "mini-css-extract-plugin": "^0.4.3",
        "moment": "^2.22.2",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss": "^7.0.2",
        "postcss-extend": "^1.0.5",
        "postcss-hexrgba": "^1.0.1",
        "postcss-import": "^12.0.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.1.0",
        "postcss-nested-ancestors": "^2.0.0",
        "postcss-simple-vars": "^5.0.1",
        "purgecss-webpack-plugin": "^1.3.0",
        "purgecss-whitelister": "^2.2.0",
        "resolve-url-loader": "^3.0.0",
        "terser-webpack-plugin": "^1.1.0",
        "vue-loader": "^15.4.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.19.1",
        "webpack-bundle-analyzer": "^3.0.2",
        "webpack-cli": "^3.1.1",
        "webpack-dev-server": "^3.1.9",
        "webpack-manifest-plugin": "^2.0.4",
        "webpack-merge": "^4.1.4",
        "webpack-notifier": "^1.6.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "axios": "^0.18.0",
        "datatables": "^1.10.18",
        "tailwindcss": "^0.6.6",
        "vue": "^2.5.17",
        "vue-apexcharts": "^1.1.0",
        "vue-confetti": "^0.4.2",
        "vue-events": "^3.1.0",
        "vuedraggable": "^2.16.0",
        "vuetable-2": "^1.7.0"
    }
}
