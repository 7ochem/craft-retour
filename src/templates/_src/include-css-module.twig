{% macro includeCssModule(cssModuleName) -%}
    {% set hashLookup = {
        <% var obj = htmlWebpackPlugin.files.chunks; %>
        <% Object.keys(obj).forEach(function(key) { %>
            <% if (obj[key].css != "") { %>
            "<%= key %>": "<%= obj[key].css %>",
            <% } %>
        <% }); %>
    } %}
    <link rel="stylesheet" href="{{ hashLookup[cssModuleName] }}" />
{%- endmacro includeCssModule %}
