{% extends "_layouts/cp" %}

{% from 'retour/_includes/macros.twig' import registerJsModules %}

{% block head %}
    {{ parent() }}
    {{ craft.retour.includeCssModule("vendors~dashboard~redirects.css", false) }}
    {{ craft.retour.includeCssModule("styles.css", false) }}
{% endblock %}

{% block foot %}
    {{ parent() }}
    {% set jsModules = [
        'vendors~dashboard~redirects.js',
        'styles.js',
        'retour.js',
    ] %}
    {{ registerJsModules(jsModules) }}
{% endblock %}
